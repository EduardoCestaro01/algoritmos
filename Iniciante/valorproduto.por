programa {
  funcao inicio() {
    //declaracao de variaveis
    real produto, valor
    inteiro quantidade 
    //entrada
    escreva("quanto custa o produto:")
    leia(produto)
    escreva("quantos produtos voce quer:")
    leia(quantidade)
    //processamento
    valor = produto*quantidade
    //Saída
    escreva("o valor vai ser:", valor)
  
    
  }
}
